{
    "name": "prezent/grid-bundle",
    "type": "symfony-bundle",
    "description": "Generic data grids and lists using prezent-grid",
    "keywords": ["grid", "datagrid", "list", "table"],
    "homepage": "http://www.prezent.nl",
    "require": {
        "php": "^5.5|^7.0",
        "ext-intl": "*",
        "prezent/grid": ">=0.8 <0.10",
        "symfony/dependency-injection": "^2.6|^3.0|^4.0",
        "symfony/framework-bundle": "^2.6|^3.0|^4.0",
        "symfony/options-resolver": "^2.6|^3.0|^4.0",
        "symfony/translation": "^2.3|^3.0|^4.0",
        "symfony/twig-bundle": "^2.3|^3.0|^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1"
    },
    "suggest": {
        "dms/twig-extension-bundle": "A bundle to automatically load the Twig Extensions into your application"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander Marechal",
            "email": "sander@prezent.nl"
        },
        {
            "name": "Prezent Internet B.V.",
            "email": "github@prezent.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Prezent\\GridBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
