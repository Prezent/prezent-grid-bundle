{
    "name": "prezent/grid-bundle",
    "type": "symfony-bundle",
    "description": "Generic data grids and lists using prezent-grid",
    "keywords": ["grid", "datagrid", "list"],
    "homepage": "http://www.prezent.nl",
    "require": {
        "php": "~5.5",
        "ext-intl": "*",
        "prezent/grid": "~0.3.0",
        "symfony/framework-bundle": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/twig-bundle": "~2.3"
    },
    "suggest": {
        "dms/twig-extension-bundle": "A bundle to autmatically load the Twig Extensions into your application"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander Marechal",
            "email": "sander@prezent.nl"
        },
        {
            "name": "Prezent Internet B.V.",
            "email": "github@prezent.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Prezent\\GridBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
