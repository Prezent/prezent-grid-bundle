{
    "name": "prezent/grid-bundle",
    "type": "symfony-bundle",
    "description": "Generic data grids and lists using prezent-grid",
    "keywords": ["grid", "datagrid", "list", "table"],
    "homepage": "http://www.prezent.nl",
    "require": {
        "php": "^7.0|^8.0",
        "ext-intl": "*",
        "prezent/grid": "^0.12|^0.13|^0.14",
        "symfony/dependency-injection": "^5.0|^6.0",
        "symfony/framework-bundle": "^5.0|^6.0",
        "symfony/options-resolver": "^5.0|^6.0",
        "symfony/translation": "^5.0|^6.0",
        "symfony/twig-bundle": "^5.0|^6.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^5.0|^6.0",
        "symfony/css-selector": "^5.0|^6.0",
        "symfony/finder": "^5.0|^6.0",
        "symfony/phpunit-bridge": "^5.0|^6.0",
        "symfony/templating": "^5.0|^6.0",
        "symfony/yaml": "^5.0|^6.0"
    },
    "suggest": {
        "twig/extra-bundle": "Automatically load twig extra extensions"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander Marechal",
            "email": "sander@prezent.nl"
        },
        {
            "name": "Prezent Internet B.V.",
            "email": "github@prezent.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Prezent\\GridBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
